Algoritmo "senha_fixa"

Var

   senhaInserida: inteiro

Inicio

   escreva("Digite a senha: ")
   leia(senhaInserida)
   
   enquanto senhaInserida <> 2002 faca
      escreva("Senha inválida! Tente novamente: ")
      leia(senhaInserida)
   fimenquanto
   
   escreval("Acesso permitido!")

Fimalgoritmo